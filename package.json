{
  "name": "@evo-mark/colour-convert",
  "description": "Plain colour conversion functions",
  "version": "3.0.0",
  "author": "Josh Junon <josh@junon.me>",
  "contributors": [
    "Heather Arthur <fayearthur@gmail.com>",
    "Craig Riley <c.riley@evomark.co.uk>"
  ],
  "license": "MIT",
  "repository": "evo-mark/colour-convert",
  "scripts": {
    "pretest": "xo",
    "test": "node test/basic.js"
  },
  "engines": {
    "node": "^18.0.0|^20.0.0|^21.0.0|^22.0.0"
  },
  "type": "module",
  "keywords": [
    "color",
    "colour",
    "convert",
    "converter",
    "conversion",
    "rgb",
    "hsl",
    "hsv",
    "hwb",
    "cmyk",
    "ansi",
    "ansi16"
  ],
  "files": [
    "index.js",
    "conversions.js",
    "route.js"
  ],
  "xo": {
    "rules": {
      "default-case": 0,
      "no-inline-comments": 0,
      "operator-linebreak": 0,
      "unicorn/prefer-exponentiation-operator": 0
    }
  },
  "devDependencies": {
    "chalk": "^2.4.2",
    "jimp": "^0.16.1",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "color-name": "~1.1.4"
  }
}
